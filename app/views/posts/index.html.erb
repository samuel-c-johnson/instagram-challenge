<!DOCTYPE html>
 <html>
   <head>
     <meta charset="utf-8">
     <title>
     </title>
   </head>
   <body>
     <h1>Instagram</h1>
     <%= link_to "new post", new_post_path %>

     <p>

      <% @posts.each do |post| %>
        <tr>
          <td><%= link_to(image_tag(post.image.url(:medium)), post_path(post)) %> </td>
          <p>
          <td><%= post.title %></td>
          <p>
            <td><%= link_to 'Edit', edit_post_path(post) %></td>
          </p>

        </tr>
      <% end %>
    </p>

   </body>
 </html>
